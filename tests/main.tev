$c * -> Allow to stack datas localy
$stack[name;Morgan] $c -> test
$log[$get[name]] $c -> test

$c ? -> Stock datas from others functions.
$stack[ping;$process[ping]]
$log[ $get[ping] ]

$c ? -> Informations obtainable with $process
$log[$process[nodeVersion]]
$log[$process[npmVersion]]
$log[$process[ping]]
$log[$process[tevVersion]]

$c ? -> Informations obtainable with $OS
$log[$OS[type]]
$log[$OS[version]]
$log[$OS[uptime]]
$log[$OS[totalMemory]]
$log[$OS[freeMemory]]
$log[$OS[cpuModel]]
$log[$OS[cpuSpeed]]
$log[$OS[architecture]]
$log[$ping]
$log[$OS[platform]] $c -> test

$if[1==1] $c -> test
    $log[Yes]
$elseif[1==2] $c -> test
    $log[Yes elseif]
$else
    $log[No]
$endif

